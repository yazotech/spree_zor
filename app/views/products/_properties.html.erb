<% unless @product_properties.empty? %>
<dl id="product-properties" class="table-display" data-hook>
<% for product_property in @product_properties 
    next if product_property && product_property.value.to_s.slice(0,6) == '<embed'
  	song_name, song_url = product_property.value.split(',')
%>
    <dd><%= link_to song_name, '/songs/' + song_url if song_url %></dd>

	<% end %>
</dl>
<% end %>
